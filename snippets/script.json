{
  "DataSet-import": {
    "prefix": "importds",
    "body": [
      "import { DataSet } from \"choerodon-ui/pro\";"
    ],
    "description": "从 choerodon-ui/pro 引入 DataSet"
  },
  "DataSet-new": {
    "prefix": "newds",
    "body": [
      "const ${1:dataSet} = new DataSet(${2:newDataSet})"
    ],
    "description": "新建 DataSet"
  },
  "DataSet-init": {
    "prefix": "initds",
    "body": [
      "const ${1:newDataSet} = {",
      "\tautoQuery: ${2|true,false|},",
      "\tautoCreate: ${3|true,false|},  ",
      "\tselection: ${4|'single','multiple',false|},",
      "\tpaging: ${5|false,true|},",
      "\tpageSize: ${6:10},",
      "\tqueryFields: [",
      "\t\t",
      "\t],",
      "\tfields: [",
      "\t\t",
      "\t],",
      "\ttransport: {",
      "\t\t",
      "\t},",
      "\tevents: {",
      "\t\t",
      "\t}$0",
      "}"
    ],
    "description": "初始化 DataSet 结构"
  },
  "DataSet-queryField-init": {
    "prefix": "newQuery",
    "body": [
      "{", 
      "\tname: '${1}',", 
      "\ttype: '${2|boolean,number,string,date,dateTime,time,week,month,year,email,url,intl,object|}',",  
      "\tlabel: '${3}',",
      "}$0"
    ],
    "description": "新建一条 queryField"
  },
  "DataSet-field-init": {
    "prefix": "newField",
    "body": [
      "{", 
      "\tname: '${1}',", 
      "\ttype: '${2|boolean,number,string,date,dateTime,time,week,month,year,email,url,intl,object|}',",  
      "\tlabel: '${3}',",
      "}$0"
    ],
    "description": "新建一条 field"
  }
}